create table teachers(TID varchar(100), TNAME varchar(200), TPASS varchar(100), CLASS varchar(100), YEAR varchar(100), isApproved int, primary key (TID));



CREATE TABLE 1A (gr_no INT, roll_no INT, curr_std VARCHAR(15), division VARCHAR(15), full_name VARCHAR(255), father VARCHAR(100), mother VARCHAR(100), birth_date VARCHAR(100), ENGLISH_pt1 INT, ENGLISH_nb1 INT, ENGLISH_sea1 INT, ENGLISH_HY INT, ENGLISH_pt2 INT, ENGLISH_nb2 INT, ENGLISH_sea2 INT, ENGLISH_AE INT, HINDI_pt1 INT, HINDI_nb1 INT, HINDI_sea1 INT, HINDI_HY INT, HINDI_pt2 INT, HINDI_nb2 INT, HINDI_sea2 INT, HINDI_AE INT, MATHS_pt1 INT, MATHS_nb1 INT, MATHS_sea1 INT, MATHS_HY INT, MATHS_pt2 INT, MATHS_nb2 INT, MATHS_sea2 INT, MATHS_AE INT, EVS_pt1 INT, EVS_nb1 INT, EVS_sea1 INT, EVS_HY INT, EVS_pt2 INT, EVS_nb2 INT, EVS_sea2 INT, EVS_AE INT, GUJARATI_pt1 INT, GUJARATI_nb1 INT, GUJARATI_sea1 INT, GUJARATI_HY INT, GUJARATI_pt2 INT, GUJARATI_nb2 INT, GUJARATI_sea2 INT, GUJARATI_AE INT, COMPUTER_pt1 INT, COMPUTER_nb1 INT, COMPUTER_sea1 INT, COMPUTER_HY INT, COMPUTER_pt2 INT, COMPUTER_nb2 INT, COMPUTER_sea2 INT, COMPUTER_AE INT, PRIMARY KEY (gr_no), FOREIGN KEY (gr_no, full_name, father, mother, birth_date) REFERENCES gr_details(GR_NO, NAME, FATHER, MOTHER, BIRTH_DATE), FOREIGN KEY (gr_no, curr_std, division) REFERENCES academic_detail(gr_no, curr_std, division));


